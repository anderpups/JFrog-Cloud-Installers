###########################################################
## this configuration was generated by JFrog Artifactory ##
###########################################################
  ## add HA entries when ha is configure
  upstream artifactory {
  server 127.0.0.1:8082;
}
  upstream artifactory-direct {
  server 127.0.0.1:8081;
}
  ssl_protocols TLSv1.1 TLSv1.2 TLSv1.3;
  ssl_certificate      /var/opt/jfrog/nginx/ssl/cert.pem;
  ssl_certificate_key  /etc/pki/tls/cert.key;
  ssl_session_cache shared:SSL:1m;
  ssl_prefer_server_ciphers   on;
  ## server configuration
  server {
  listen 443 ssl http2;
{% if (docker_subdomain_enabled | bool) %}
  server_name ~(?<repo>.+)\.{{ server_name }} {{ server_name }};
{% elif not (docker_subdomain_enabled | bool) %}
  server_name {{ server_name }};
{% endif %}
  if ($http_x_forwarded_proto = '') {
    set $http_x_forwarded_proto  $scheme;
  }
  ## Application specific logs
  access_log /var/log/nginx/artifactory-access.log;
  error_log /var/log/nginx/artifactory-error.log;
  rewrite ^/$ /ui/ redirect;
  rewrite ^/ui$ /ui/ redirect;
{% if (docker_subdomain_enabled | bool) %}
  rewrite ^/(v1|v2)/(.*) /artifactory/api/docker/$repo/$1/$2;
  rewrite ^/.well-known/terraform.json /artifactory/api/terraform/$repo/.well-known/terraform.json;
{% endif %}
  chunked_transfer_encoding on;
  client_max_body_size 0;
  location / {
  proxy_read_timeout  2400s;
  proxy_pass_header   Server;
  proxy_cookie_path   ~*^/.* /;
  proxy_pass          "http://artifactory";
  proxy_next_upstream error timeout non_idempotent;
  proxy_next_upstream_tries    1;
  proxy_buffer_size 128k;
  proxy_buffers 40 128k;
  proxy_busy_buffers_size 128k;
  proxy_set_header    X-JFrog-Override-Base-Url $http_x_forwarded_proto://$host:$server_port;
  proxy_set_header    X-Forwarded-Port  $server_port;
  proxy_set_header    X-Forwarded-Proto $http_x_forwarded_proto;
  proxy_set_header    Host              $http_host;
  proxy_set_header    X-Forwarded-For   $proxy_add_x_forwarded_for;
  add_header X-Content-Type-Options "nosniff" always;
  add_header Strict-Transport-Security always;
  location ~ ^/artifactory/ {
    proxy_pass    http://artifactory-direct;
  }
  }
}
